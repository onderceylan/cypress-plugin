{
  "name": "@onderceylan/cypress-plugin",
  "description": "Cypress plugin for ng-apimock",
  "version": "1.0.1",
  "homepage": "https://github.com/ng-apimock/cypress-plugin#readme",
  "author": {
    "name": "Mischa Dasberg",
    "email": "mischa@dasberg.nl"
  },
  "main": "dist/index.js",
  "types": "dist/cypress.d.ts",
  "scripts": {
    "start": "node test/server.js",
    "precompile": "rm -rf dist",
    "compile": "tsc -p tsconfig.json",
    "postcompile": "copyfiles -u 1 src/**/*.d.ts dist",
    "lint": "tslint -p tsconfig.json",
    "test": "jasmine-ts --config=jasmine.json",
    "cypress-local": "cypress open",
    "cypress-ci": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-apimock/cypress-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/ng-apimock/cypress-plugin/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ng-apimock/cypress-plugin/blob/master/LICENSE-MIT"
    }
  ],
  "keywords": [
    "apimock",
    "ng-apimock",
    "http",
    "api",
    "cypress",
    "plugin"
  ],
  "devDependencies": {
    "@ng-apimock/core": "1.0.23",
    "@ng-apimock/test-application": "1.0.17",
    "@types/chai": "4.2.3",
    "@types/connect": "3.4.32",
    "@types/fs-extra": "8.0.1",
    "@types/jasmine": "3.4.4",
    "@types/node": "13.13.4",
    "@types/sinon": "7.5.0",
    "@types/url-join": "4.0.0",
    "@types/uuid": "3.4.5",
    "bluebird": "3.7.2",
    "chai": "4.2.0",
    "connect": "3.7.0",
    "copyfiles": "2.2.0",
    "cypress": "4.5.0",
    "cypress-cucumber-preprocessor": "2.3.1",
    "eventemitter2": "6.3.1",
    "fs-extra": "8.1.0",
    "jasmine": "3.5.0",
    "jasmine-ts": "0.3.0",
    "serve-static": "1.14.1",
    "sinon": "7.5.0",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.6.4",
    "vrsource-tslint-rules": "6.0.0"
  },
  "dependencies": {
    "@ng-apimock/base-client": "1.0.14",
    "urljoin": "0.1.5"
  },
  "peerDependencies": {
    "cypress": "4.x"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "test/step_definitions"
  }
}